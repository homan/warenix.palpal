<LinearLayout android:id="@+id/linearLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical">
	<TextView android:text="TextView" android:id="@+id/socialNetworkName"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textSize="20dip" android:textColor="#FFFFFFFF"
		android:textStyle="bold"></TextView>
	<TextView android:text="TextView" android:id="@+id/nick"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textSize="16dip" android:textColor="#FFFFFFFF"></TextView>
</LinearLayout>

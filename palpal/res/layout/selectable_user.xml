<RelativeLayout android:id="@+id/RelativeLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_margin="5dip">
	<CheckBox android:id="@+id/checkbox" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_centerVertical="true" />
	<org.dyndns.warenix.widget.WebImage
		android:id="@+id/profileImage" android:layout_height="64dip"
		android:layout_toRightOf="@id/checkbox" android:layout_width="64dip"
		android:src="@drawable/icon" android:layout_centerVertical="true" />
	<TextView android:id="@+id/username" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_toRightOf="@id/profileImage"
		android:text="" android:layout_centerVertical="true" android:textSize="20dip"
		android:textColor="#FFffffff" android:layout_marginLeft="10dip" />
</RelativeLayout>
